    "@hashgraph/sdk": "2.29.0",
    "@stablelib/sha384": "^1.0.0",
    "bignumber.js": "^9.0.0",
    "lodash": "^4.17.15",
    "long": "^4.0.0",
    "protobufjs": "7.2.5",
    "stellar-sdk": "^10.0.1",
    "tweetnacl": "^1.0.3"
  },
  "devDependencies": {
    "@bitgo/sdk-api": "^1.63.3",
