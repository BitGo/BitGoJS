{
  "name": "@bitgo/utxo-lib",
  "version": "2.2.1",
  "description": "Client-side Bitcoin JavaScript library",
  "main": "./dist/src/index.js",
  "engines": {
    "node": ">=10.22.0 <17",
    "npm": ">=3.10.10"
  },
  "keywords": [
    "bitgo",
    "utxo",
    "bitcoinjs",
    "bitcoin",
    "zcash",
    "dash",
    "browserify",
    "javascript"
  ],
  "scripts": {
    "prepare": "npm run build",
    "build": "tsc --project tsconfig.build.json",
    "coverage-report": "nyc report --reporter=lcov",
    "coverage-html": "nyc report --reporter=html",
    "coverage": "npm run build && nyc --check-coverage mocha",
    "test": "npm run coverage",
    "unit-test": "mocha",
    "lint": "eslint --quiet .",
    "fmt": "prettier --write '{src,test}/**/*.{ts,js}'"
  },
  "nyc": {
    "include": [
      "src/**"
    ],
    "branches": 78,
    "functions": 90,
    "lines": 88
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BitGo/BitGoJS.git",
    "directory": "modules/utxo-lib"
  },
  "files": [
    "dist/src"
  ],
  "dependencies": {
    "@bitgo/blake2b": "^3.0.2",
    "@noble/secp256k1": "git+https://github.com/brandonblack/noble-secp256k1#601438008aa532334cf403948850d56966f34f36",
    "bip32": "^3.0.1",
    "bitcoin-ops": "^1.3.0",
    "bitcoinjs-lib": "git+https://github.com/BitGo/bitcoinjs-lib#0afe94d5718c034a5c33595500585ad52add2524",
    "bs58check": "^2.1.2",
    "cashaddress": "^1.1.0",
    "create-hmac": "^1.1.7",
    "ecpair": "^2.0.1",
    "typeforce": "^1.11.3",
    "varuint-bitcoin": "^1.1.2"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.12",
    "@types/node": "^14.18.12",
    "axios": "^0.21.1",
    "debug": "^3.1.0",
    "fs-extra": "^9.1.0",
    "nyc": "^15.1.0",
    "ts-node": "^9.1.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT"
}
