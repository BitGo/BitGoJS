    "url": "https://github.com/BitGo/BitGoJS.git",
    "directory": "modules/sdk-coin-icp"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "yarn prettier --write",
      "yarn eslint --fix"
    ]
