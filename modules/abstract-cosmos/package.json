    "@cosmjs/proto-signing": "^0.29.5",
    "@cosmjs/stargate": "^0.29.5",
    "bignumber.js": "^9.1.1",
    "cosmjs-types": "^0.6.1",
    "lodash": "^4.17.21",
    "protobufjs": "^7.4.0",
