  "description": "BitGo JavaScript SDK",
  "main": "./dist/src/index.js",
  "types": "./dist/types/src/index.d.ts",
  "keywords": [
    "bitgo",
    "bitcoin",
    "bitgojs"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/BitGo/BitGoJS.git",
    "directory": "modules/bitgo"
