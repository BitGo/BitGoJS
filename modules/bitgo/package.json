{
  "name": "bitgo",
  "version": "14.2.0-rc.23",
  "description": "BitGo JavaScript SDK",
  "main": "./dist/src/index.js",
  "types": "./dist/types/src/index.d.ts",
  "keywords": [
    "bitgo",
    "bitcoin",
    "bitgojs"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/BitGo/BitGoJS.git",
    "directory": "modules/bitgo"
  },
  "license": "Apache-2.0",
  "engines": {
    "node": ">=14 <17",
    "npm": ">=3.10.10"
  },
  "scripts": {
    "compile": "npm run webpack-prod",
    "compile-dbg": "npm run webpack-dev",
    "compile-test": "npm run webpack-dev",
    "webpack-dev": "cross-env NODE_ENV=development webpack",
    "webpack-prod": "cross-env NODE_ENV=production webpack",
    "test": "npm run coverage",
    "unit-test": "mocha",
    "coverage": "nyc -- npm run unit-test",
    "integration-test": "nyc -- mocha \"test/v2/integration/**/*.ts\"",
    "browser-test": "karma start karma.conf.js",
    "lint": "eslint --quiet .",
    "audit": "if [ \"$(npm --version | cut -d. -f1)\" -ge \"6\" ]; then npm audit; else echo \"npm >= 6 required to perform audit. skipping...\"; fi",
    "clean": "rm -r dist/*",
    "build": "yarn tsc --build --incremental --verbose .",
    "prepare": "npm run build",
    "prepublishOnly": "npm run compile",
    "upload-artifacts": "node scripts/upload-test-reports.js",
    "check-fmt": "prettier --check '{src,test}/**/*.{ts,js,json}'",
    "precommit": "yarn lint-staged",
    "unprettied": "grep -R -L --include '*.ts' --include '*.js' --include '*.json' '@prettier' src test",
    "fmt": "prettier --write '{src,test}/**/*.{ts,js,json}'",
    "upload-docs": "node scripts/upload-docs.js",
    "gen-docs": "typedoc"
  },
  "dependencies": {
    "@bitgo/abstract-eth": "^1.0.1-rc.5",
    "@bitgo/abstract-utxo": "^1.0.1-rc.5",
    "@bitgo/account-lib": "^2.20.0-rc.20",
    "@bitgo/blockapis": "^1.0.1-rc.14",
    "@bitgo/sdk-api": "^1.1.0-rc.16",
    "@bitgo/sdk-coin-algo": "^1.0.1-rc.0",
    "@bitgo/sdk-coin-cspr": "^1.0.1-rc.0",
    "@bitgo/sdk-coin-xrp": "^1.1.0-rc.6",
    "@bitgo/sdk-core": "^1.1.0-rc.13",
    "@bitgo/sjcl": "^1.0.1-rc.3",
    "@bitgo/statics": "^6.18.0-rc.15",
    "@bitgo/unspents": "^0.8.0-rc.12",
    "@bitgo/utxo-lib": "^2.3.0-rc.8",
    "@types/bluebird": "^3.5.25",
    "@types/superagent": "^4.1.3",
    "assert": "^2.0.0",
    "bigi": "^1.4.0",
    "bignumber.js": "^8.0.1",
    "bip32": "^2.0.6",
    "bitcoinjs-message": "^2.0.0",
    "bluebird": "^3.5.3",
    "bs58": "^2.0.1",
    "bs58check": "^1.0.4",
    "cashaddress": "^1.1.0",
    "debug": "^3.1.0",
    "ecurve": "^1.0.6",
    "eol": "^0.5.0",
    "eosjs": "^21.0.2",
    "eosjs-ecc": "^4.0.4",
    "keccak": "^3.0.2",
    "lodash": "^4.17.14",
    "moment": "^2.20.1",
    "openpgp": "5.1.0",
    "sanitize-html": "^1.27.5",
    "secp256k1": "^4.0.2",
    "secrets.js-grempe": "^1.1.0",
    "stellar-sdk": "^10.0.1",
    "superagent": "^3.8.3",
    "superagent-proxy": "^3.0.0"
  },
  "devDependencies": {
    "@bitgo/sdk-test": "^1.0.1-rc.2",
    "@types/create-hmac": "^1.1.0",
    "@types/debug": "^4.1.4",
    "@types/jasmine": "^3.5.12",
    "@types/lodash": "^4.14.121",
    "@types/secp256k1": "^4.0.1",
    "aws-sdk": "^2.1155.0",
    "html-webpack-plugin": "^5.5.0",
    "https-browserify": "^1.0.0",
    "karma": "^5.1.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-jasmine": "^4.0.0",
    "karma-mocha": "^2.0.1",
    "karma-typescript": "^5.0.3",
    "minimist": "^1.2.5",
    "nyc": "^15.0.0",
    "process": "^0.11.10",
    "puppeteer": "2.1.1",
    "q": "^1.1.2",
    "request": "^2.88.0",
    "request-promise": "^4.2.2",
    "sjcl": "^1.0.8",
    "supertest": "^3.0.0",
    "terser": "^3.16.1"
  },
  "optionalDependencies": {
    "@ethereumjs/common": "^2.4.0",
    "@ethereumjs/tx": "^3.3.0",
    "ethereumjs-abi": "^0.6.5",
    "ethereumjs-util": "6.2.1"
  },
  "nyc": {
    "extension": [
      ".ts"
    ]
  },
  "lint-staged": {
    "*.{js,ts}": [
      "yarn prettier --write",
      "yarn eslint --fix"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
